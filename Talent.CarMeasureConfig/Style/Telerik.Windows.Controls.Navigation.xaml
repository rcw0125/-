                <Border BorderThickness="{StaticResource ToolBar_ToggleButton_InnerBorderThickness}" CornerRadius="0 2 2 0" BorderBrush="{StaticResource ToolBar_ToggleButton_normalInnerBorder}"/>
            </Border>
